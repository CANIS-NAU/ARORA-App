<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MindfullnessSelection"
    android:background="@drawable/background"
    >

    <ImageView
        android:id="@+id/user_butterfly_imageView"
        android:layout_width="255dp"
        android:layout_height="234dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="76dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/orange_butterfly_image" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="68dp"
        android:layout_height="68dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="100dp"
        android:layout_weight="1"
        android:background="#00000000"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/user_butterfly_imageView"
        app:srcCompat="@drawable/location_button" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="68dp"
        android:layout_height="68dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="172dp"
        android:background="#00000000"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/user_butterfly_imageView"
        app:srcCompat="@drawable/breath_button" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="68dp"
        android:layout_height="68dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="60dp"
        android:background="#00000000"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/user_butterfly_imageView"
        app:srcCompat="@drawable/home_button" />

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="57dp"
        android:layout_height="46dp"
        android:layout_marginEnd="176dp"
        android:layout_marginBottom="28dp"
        android:background="#00000000"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/home_button" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="63dp"
        android:layout_height="16dp"
        android:layout_marginEnd="172dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/dots_left" />


</android.support.constraint.ConstraintLayout>